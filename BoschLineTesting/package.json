{
  "version": "1.0.0",
  "name": "bosch-line-testing",
  "private": true,
  "scripts": {
    "start": "gulp scriptsNStyles && npm run tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "docker-build": "docker build -t bosch-line-testing .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 bosch-line-testing",
    "pree2e": "npm run webdriver:update",
    "e2e": "npm run tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server -c ./lite-server-root/bsConf.js",
    "postinstall": "typings install",
    "test": "npm run tsc && concurrently \"npm run tsc:w\" \"karma start karma.conf.js\"",
    "test-once": "npm run tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc -p ./tsScripts/tsconfig.json",
    "tsc:w": "tsc -w -p ./tsScripts/tsconfig.json",
    "typings": "typings",
    "webdriver:update": "webdriver-manager update"
  },
  "dependencies": {
    "@angular/common": "^2.2.0",
    "@angular/compiler": "^2.2.0",
    "@angular/core": "^2.2.0",
    "@angular/forms": "^2.2.0",
    "@angular/http": "^2.2.0",
    "@angular/material": "^2.0.0-alpha.11",
    "@angular/platform-browser": "^2.2.0",
    "@angular/platform-browser-dynamic": "^2.2.0",
    "@angular/router": "^3.2.0",
    "@angular/upgrade": "^2.2.0",
    "@angular2-material/core": "^2.0.0-alpha.11",
    "angular2-in-memory-web-api": "^0.0.20",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "immutable": "^3.8.1",
    "modernizr": "^3.3.1",
    "ng2-auto-complete": "^0.7.4",
    "ng2-datetime-picker": "^0.9.10",
    "ng2-material": "^0.8.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "^5.0.0-rc.3",
    "systemjs": "^0.19.27",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/core-js": "^0.9.34",
    "@types/hammerjs": "^2.0.33",
    "@types/immutable": "^3.8.5",
    "@types/jasmine": "^2.5.37",
    "@types/node": "^6.0.46",
    "@types/rx": "^2.5.33",
    "canonical-path": "0.0.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.0",
    "gulp-modernizr": "^1.0.0-alpha",
    "gulp-tsc": "^1.2.0",
    "gulp-typescript": "^2.13.6",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.2",
    "merge-stream": "^1.0.0",
    "path": "^0.12.7",
    "protractor": "^3.3.0",
    "respond.js": "^1.4.2",
    "rimraf": "^2.5.4",
    "tslint": "^3.15.1",
    "typescript": "^2.1.1",
    "typings": "^1.3.2"
  }
}
